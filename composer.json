{
    "name": "farukh/geo-locator",
    "type": "library",
    "description": "Detect geo location by IP address.",
    "authors": [
        {
            "name": "Farukh Narzullaev",
            "email": "faruh.narzullaev@bk.ru"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Farran\\Geo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farran\\Geo\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "dev-master",
        "laminas/laminas-diactoros": "2.3.*",
        "monolog/monolog": "^2.0",
        "psr/http-client": "1.0.*",
        "psr/log": "1.1.*",
        "psr/simple-cache": "1.0.*",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
